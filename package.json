{
  "name": "@civ-clone/electron-renderer",
  "version": "0.1.0",
  "main": "main.js",
  "repository": "git@github.com:civ-clone/electron-renderer.git",
  "keywords": [
    "typescript",
    "civilization"
  ],
  "author": "dom111 <dom111@users.noreply.github.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm run prettier:format && npm run ts:compile",
    "prettier:check": "prettier --config .prettierrc '**/*.ts'",
    "prettier:format": "prettier --config .prettierrc '**/*.ts' --write",
    "start": "electron .",
    "test": "ts-mocha ./tests/*.test.ts",
    "test:coverage": "c8 ts-mocha ./tests/*.test.ts",
    "ts:compile": "tsc --build tsconfig.json"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "c8": "^7.3.1",
