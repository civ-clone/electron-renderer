    "ts-mocha": "^7.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@civ-clone/civ1-city": "github:civ-clone/civ1-city",
    "@civ-clone/civ1-city-happiness": "github:civ-clone/civ1-city-happiness",
